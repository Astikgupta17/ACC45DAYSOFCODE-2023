#include <iostream>
using namespace std;

int main() {
		int t,n;
		cin>>t;
		while(t--){
		    cin>>n;
		    int arr[n];
		    for(int i =0; i < n; i++){
		        cin>>arr[i];
		    }
		    int count = 0;
		    for(int i =0; i < n; i++){
		        if(arr[i]%2 != 0){
		            count++;
		        }
		    }
		    if(count >= 2 && count%2 == 0){
		        cout<<"YES"<<endl;
		    } else{
		        cout<<"NO"<<endl;
		    }
		}
	return 0;
}
