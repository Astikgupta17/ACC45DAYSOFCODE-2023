#include <iostream>
#include<string.h>
using namespace std;

int main() {
	int i,t,d,p;
	string n;
	cin>>t;
	while(t--){
	    cin>>d;
	    cin>>n;
	    p=0;
	    for(i=0;i<d;i++){
	     if(n[i]=='0' || n[i]=='5')	     
	     ++p;
	 }   
	 
	 if(p>0)
	 cout<<"Yes"<<endl;
	 
	 else
	 cout<<"No"<<endl;
	}

	return 0;
}
