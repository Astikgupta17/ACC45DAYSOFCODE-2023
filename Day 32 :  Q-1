#include <iostream>
using namespace std;

int main() {
	int t,x,n,i,p;
	cin>>t;
	while(t--){
	    cin>>n>>x;
	    int A[n];
	    
	    p=0;
	    for(i=0;i<n;i++)
	    cin>>A[i];
	    
	    for(i=0;i<n;i++){
	       if(A[i]>=x)
	       p+=1;
	       
	       else
	       p+=0;
	    }
	    
	    cout<<p<<endl;
	}
	return 0;
}
