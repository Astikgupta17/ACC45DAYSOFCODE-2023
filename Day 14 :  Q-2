#include <iostream>
using namespace std;

int main() {
	 int t,x,y,z;
	 cin>>t;
	 while(t--){
	     cin>>x>>y>>z;
	  
	  if((x+y-z==0) || (x-y+z==0) || (-x+y+z==0)){
	     if(x=y=z!=0)
	     cout<<"yes"<<endl;
	     
	     else
	     cout<<"no"<<endl;
	     }

	  else
	  cout<<"No"<<endl;
	 }
	return 0;
}
