#include <iostream>
using namespace std;

int main() {
	int t,i,n,b,area;
	cin>>t;
	
	while(t--){
	    cin>>n>>b;
	    int H[n],W[n],P[n];
	    
	    for(i=0;i<n;i++)
	    cin>>W[i]>>H[i]>>P[i];
	    
	   for(i=0;i<n;i++)
	    {
	        if(P[i]>b)
	        cout<<"no table"<<endl;
	        
	        else
	        cout<<W[i]*H[i]<<endl;
	    }
	}
	return 0;
}
